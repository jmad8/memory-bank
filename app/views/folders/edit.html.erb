<% provide(:title, 'Editing ' + @folder.name) %>

<%= form_for @folder, url: {action: 'update'} do |f| %>
	<%= render 'shared/error_messages', object: @folder %>
	<%= render 'fields', f: f %>
	<%= render 'shared/form_buttons', f: f, object: @folder, cancel_path: memories_path(folder: @folder.id), delete_title: @folder.name %> 
<% end %>